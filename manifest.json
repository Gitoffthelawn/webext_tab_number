{
    "manifest_version": 2,
    "name": "Tab Count",
    "version": "0.4.1",
    "description": "Shows number of open tabs.",
    "permissions": [
        "tabs",
        "storage"
    ],
    "browser_action": {
        "default_icon": "icons/ic_tab_black_24px.svg"
    },
    "background": {
        "scripts": ["getOptions.js", "background.js"]
    },
    "options_ui": {
        "page": "options.html",
        "browser_style": false
    }
}
